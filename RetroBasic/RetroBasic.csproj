    <ItemGroup>
      <Reference Include="WinUSBNet">
        <HintPath>..\..\..\..\Desktop\WinUSBNet-2.0.0\WinUSBNet.dll</HintPath>
      </Reference>
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Apps\Setup\" />

        <PXE_PxeInterface>192.168.2.109</PXE_PxeInterface>
        <PXE_EnableGDB>False</PXE_EnableGDB>
        <PXE_StartCosmosGDB>False</PXE_StartCosmosGDB>
        <Name>ISO User 001</Name>
